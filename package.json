    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^3.1.0"
  },
  "dependencies": {
    "@barba/core": "^2.9.7",
    "@finsweet/ts-utils": "^0.40.0",
    "dotenv": "^16.4.5",
    "gsap": "^3.12.5",
    "node-fetch": "^2.7.0",
    "twilio": "^5.0.4",
    "typescript": "^5.3.3"
